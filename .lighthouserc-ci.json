{
  "ci": {
    "collect": {
      "url": ["https://BGannon2.github.io/Personal-Site"],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "off",
        "color-contrast": "off",
        "tap-targets": "off"
      }
    }
  }
}
