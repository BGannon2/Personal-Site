{
  "ci": {
    "collect": {
      "url": ["http://localhost:8080/"],
      "numberOfRuns": 1,
      "startServerCommand": "node lighthouse-server.js",
      "settings": {
        "preset": "desktop"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "off",
        "color-contrast": "off",
        "tap-targets": "off"
      }
    }
  }
}
